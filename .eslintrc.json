{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "eslint-plugin-import",
        "eslint-plugin-node",
        "eslint-plugin-prettier"
    ],
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "no-unused-vars":"error",
        "camelcase":"warn",
        "semi":"warn"
    }
}
